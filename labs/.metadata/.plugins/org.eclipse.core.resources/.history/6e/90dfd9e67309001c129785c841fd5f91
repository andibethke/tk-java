package de.tk.andi;

import java.util.ArrayList;
import java.util.stream.Collectors;

public class Start {

	public static void main(String[] args) {
		String[] names=new String[6];
		
		names[0]="Iryna";
		names[1]="Fabian";
		names[2]="Jakob";
		names[3]="Elsa";
		names[4]="Dennis";
		names[5]="Andi";
		//names[6]="Andi";

		ArrayList<Integer> liste=new ArrayList();
		liste.add(2222);
		liste.add(2222);
		liste.add(2222);
		liste.add(2222);
		liste.add(2222);

		System.out.println(liste.stream().collect(Collectors.summingInt(null)));
		

	}

}
