	public static void main(String[] args) {
		String[] names= {"Iryna","Fabian","Jakob","Elsa","Dennis","Andi"};
		
		Arrays.sort(names, new MeinNachLängerSortierer());
		
		for (String n : names) {
			System.out.println(n);
		}
		
	}

}

