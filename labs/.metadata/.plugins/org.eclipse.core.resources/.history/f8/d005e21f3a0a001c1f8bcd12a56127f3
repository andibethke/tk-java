package de.andi;

public class Start {

	public static void main(String[] args) {
		A a=new A();
		a.setWert(100);
		System.out.println(a);

	}

}

class A{
	private int wert=0;

	public int getWert() {
		return wert;
	}

	public void setWert(int wert) {
		this.wert = wert;
	}
	@Override
	public String toString() {
		return String.format("Wert: %d", wert);
	}
	
}